_satellite.pushAsyncScript(function(event, target, $variables){
  $(document).ajaxComplete(function(e,x,o){
  try{
    if(o.url.indexOf('summary.sfx.json') !== -1){
      var ds = $.parseJSON( x.responseText );
      //console.log(ds.locationHeavyContainerList[1].stationId);
      _satellite.setCookie('_resDrop', ds.locationHeavyContainerList[1].stationId);
    }
  }catch(e){}
});
});
